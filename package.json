{
  "name": "resistro",
  "version": "1.0.0",
  "description": "a resistor webapp",
  "main": "webpack.config.js",
  "dependencies": {
    "axios": "^0.13.1",
    "history": "^2.1.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^3.3.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build:dev": "webpack --env=dev --progress --profile --colors --bail",
    "build:prod": "webpack --env=prod --progress --profile --colors --bail",
    "build": "NODE_ENV=production webpack -p",
    "clean:dist": "rm -rf dist/",
    "clean:node": "rm -rf node_modules/",
    "clean:hard": "npm run clean:dist && npm run clean:node",
    "clean": "npm run clean:dist",
    "start": "./node_modules/.bin/webpack-dev-server --content-base src --inline --hot --port 8000",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylanpark/resistro.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dylanpark/resistro/issues"
  },
  "homepage": "https://github.com/dylanpark/resistro#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.6.0",
    "react-test-renderer": "^16.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "roots": ["<rootDir>/tests/"],
    "testRegex": ".*-test\\.(js|jsx)"
  }
